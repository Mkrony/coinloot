import CompletedAllOffer from '../model/CompletedOfferModel.js';
export const CompletedOffers = async (req, res) => {
    try {
        const offers = await CompletedAllOffer.find();
        res.status(200).json(offers);
    } catch (error) {
        console.error('Error fetching completed offers:', error);
        res.status(500).send('Error fetching completed offers');
    }
};